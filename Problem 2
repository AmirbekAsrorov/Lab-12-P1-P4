#include <iostream>
#include <string>
#include <vector>
using namespace std;

int hashCodeString(const string& s) {
    const int a = 33;
    int hash = 0;

    for (int i = 0; i < s.length(); ++i) {
        hash = hash * a + (unsigned char)s[i];
    }

    return hash;
}

int main() {
    int numStrings;
    cout << "Enter the number of strings: ";
    cin >> numStrings;

    vector<string> inputs(numStrings);

    cout << "Enter " << numStrings << " strings:\n";
    for (int i = 0; i < numStrings; i++) {
        cin >> inputs[i];
    }
    
    for (const string& str : inputs) {
        cout << str << " -> " << hashCodeString(str) << endl;
    }

    return 0;
}

